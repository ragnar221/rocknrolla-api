{
    "version":2,
    "builds": [{
        "src": "app.ts",
        "use": "@vercel/node"
    }],
    
        "headers": [
            {
                "source":"api/(./*)",
                "headers": [
                    {"key": "Access-control-allow-credentials", "value": "true"},
                    {"key": "Access-control-allow-origin","value":"true"}
                ]
            }
        ],
    

    "routes": [
        {
            "src": "/(.*)",

            "dest": "app.ts"
        }
    ]

}

